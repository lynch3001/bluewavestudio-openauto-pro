#!/bin/sh -e

set -e

case "${1}" in
  remove|purge|abort-install)
    if [ -e /etc/xdg/autostart/piwiz.desktop ]; then
        rm /etc/xdg/autostart/piwiz.desktop
    fi
    ;;

  upgrade|abort-upgrade|disappear|failed-upgrade)
    ;;

  *)
    echo "postrm called with unknown argument \`${1}'" >&2
    exit 1
    ;;
esac



exit 0
