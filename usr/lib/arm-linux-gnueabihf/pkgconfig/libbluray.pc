prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/arm-linux-gnueabihf
includedir=${prefix}/include

Name: libbluray
Description: library supporting Blu-ray playback
Version: 1.1.0
Libs: -L${libdir} -lbluray
Libs.private: -ldl -lfontconfig -lfreetype
Requires.private:  libxml-2.0 freetype2 fontconfig
Cflags: -I${includedir}
