prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/arm-linux-gnueabihf
includedir=${prefix}/include

Name: neon
Description: HTTP/WebDAV client library
Version: 0.30.2
Libs: -L${libdir} -lneon-gnutls 
Libs.private:  -lz -lgnutls -L/usr/lib/arm-linux-gnueabihf/mit-krb5 -Wl,-z,relro -lgssapi_krb5 -lkrb5 -lk5crypto -lcom_err -lxml2
Cflags: -I${includedir}/neon  -D_LARGEFILE64_SOURCE -DNE_LFS
