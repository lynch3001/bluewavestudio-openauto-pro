#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'mypy==0.670','console_scripts','stubgen'
__requires__ = 'mypy==0.670'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mypy==0.670', 'console_scripts', 'stubgen')()
    )
